<!DOCTYPE html>
<html>
<head>
<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
</head>
<body>

<h2>Users</h2>

<table class="table table-bordered">
  <thead>
      <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Age</th>
      </tr>
  </thead>
  <tbody>
      <% users.forEach(user => { %>
          <tr>
              <td><%= user.name %></td>
              <td><%= user.email %></td>
              <td><%= user.age %></td>
          </tr>
      <% }) %>

  </tbody>
</table>

</body>
</html>

